{
  "name": "@smartlyio/oats-nock-adapter",
  "version": "0.0.5",
  "license": "MIT",
  "description": "nock server adapter for smartlyio/oats s",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/src/nock.js",
  "types": "dist/src/nock.d.ts",
  "scripts": {
    "test": "yarn ts-node examples/driver.ts && yarn jest && yarn render",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rm -rf ./dist && rm -f ./tmp/*.ts",
    "build": "yarn clean && yarn lint && yarn test && yarn tsc",
    "prepublish": "yarn build",
    "render": "yarn ts-node render.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats-nock-adapter.git"
  },
  "peerDependencies": {
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@smartlyio/safe-navigation": "5.0.0",
    "lodash": "4.17.19"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats": "2.5.1",
    "@smartlyio/oats-axios-adapter": "2.2.0",
    "@smartlyio/oats-runtime": "2.6.1",
    "@types/jest": "26.0.5",
    "@types/lodash": "4.14.157",
    "@types/node": "13.13.14",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "axios": "0.19.2",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.18.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.1.0",
    "jsverify": "0.8.4",
    "nock": "13.0.2",
    "prettier": "1.19.1",
    "ts-jest": "26.1.3",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(examples|test)/.*\\.spec.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "preset": "ts-jest",
    "testMatch": null
  }
}
